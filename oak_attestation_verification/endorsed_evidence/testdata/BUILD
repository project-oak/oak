# "Legacy" here means that the testdata was added before policy-based
# verification and has no events.

package(
    default_visibility = ["//:internal"],
    licenses = ["notice"],
)

# Asserted data, the corresponding signature and the evidence are generated by
# running the ctf_sha2 tool. THe matching reference values and empty
# endorsements are handcrafted for use with InsecureAttestationVerifier.
filegroup(
    name = "testdata",
    srcs = [
        "asserted_data",
        "asserted_data_signature",
        "endorsements.binarypb",
        "evidence.binarypb",
        "reference_values.binarypb",
    ],
)
