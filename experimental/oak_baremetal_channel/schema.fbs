attribute "method_id";

rpc_service TrustedRuntime {
    HandleUserRequest(UserRequest) : UserRequestResponse (method_id: 0);
    Initialize(Initialization) : Empty (method_id: 1);
}

struct SessionId {
    value:[ubyte:8];
}

table UserRequest {
    session_id: SessionId;
    body: [ubyte];
}

table UserRequestResponse {
    body: [ubyte];
}

table Initialization {
    wasm_module: [ubyte];
}

table Empty {}
