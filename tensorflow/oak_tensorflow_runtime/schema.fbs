attribute "method_id";

rpc_service TensorflowRuntime {
    Initialize(InitializeRequest) : InitializeResponse (method_id: 0);
    Invoke(InvokeRequest) : InvokeResponse (method_id: 1);
}

table InitializeRequest {
    tensorflow_model: [ubyte];
}

table InitializeResponse {}

struct SessionId {
    value:[ubyte:8];
}

table InvokeRequest {
    session_id: SessionId;
    body: [ubyte];
}

table InvokeResponse {
    body: [ubyte];
}
